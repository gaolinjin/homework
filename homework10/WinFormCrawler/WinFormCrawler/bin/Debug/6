              <span class="float_R">版主：<p id="zidongbanzhu" style="display:inline;"></p> </span>
      <script>
      $ ("#zidongbanzhu").css("float","   ");
      var boardId= 1;
      var time = "";
      var now = new Date();
      time = time +now.getHours();
      var minute = now.getMinutes();
      if(minute<10)
      {
        time = time + "0"+minute;
      }
      else
      {
        time = time + minute;
      }
      $. ajax({
        url:'/paibanApi.do?action=banzhu',
        data:{id:boardId,time:time},
        success:function(data){
          if(/paibanName:/.test(data)){
            var banzhu = data.substr(11);
            $ ("#zidongbanzhu").html(banzhu);
          }
        }
      });
      </script>
